<% @recommendations.each do |r| %>
<p><%= cl_image_tag(r.photo) %></p>
<p>Name: <%= r.name %> </p>
<p>Address: <%= r.address %> </p>
<p>Description: <%= r.description %> </p>
<p>Category: <%= r.category %> </p>
<p>Rating: <%= r.rating %> </p>
<p> <%= link_to "show", recommendation_path(r) %> </p>
<% end %>


















